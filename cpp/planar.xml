<mujoco model='planar-biped'>
  <compiler inertiafromgeom='false' angle='radian' eulerseq='zyx'/>

  <default>
    <joint limited='true'/>
    <motor forcelimited='true'/>
    <geom contype='0' conaffinity='0' condim='1' material='geom'/>
    <!-- margin='0.001' solref='.02 1' solimp='.8 .8 .01' -->
  </default>

  <option timestep='0.002' iterations='50' solver='PGS'>
  </option>

  <size nuser_geom='1'/>

  <visual>
    <map fogstart='3' fogend='5' force='0.1' znear='0.5'/>
    <quality shadowsize='2048' offsamples='8'/>
    <global offwidth='800' offheight='800'/>
  </visual>

  <asset>
    <texture type='skybox' builtin='gradient' width='128' height='128' rgb1='.4 .6 .8' rgb2='0 0 0'/>
    <texture name='texgeom' type='cube' builtin='flat' mark='cross' width='127' height='1278' rgb1='0.8 0.6 0.4' rgb2='0.8 0.6 0.4' markrgb='1 1 1' random='0.01'/>
    <texture name='texground' type='2d' builtin='checker' rgb1='.2 .3 .4' rgb2='.1 0.15 0.2' width='512' height='512'/>
    <material name='ground' texture='texground' texrepeat='1 1' texuniform='true' reflectance='0.1'/>
    <material name='geom' rgba='0.8 0.6 0.4 1' texture='texgeom' texuniform='true'/>
  </asset>

  <worldbody>
    <!-- <geom name='floor' type='plane' pos='0 0 0' size='5 5 .125' material='MatPlane' condim='3' conaffinity='1'/> -->
    <geom type='box' pos='0 0 -0.2' size='100 1 0.2' material='ground' condim='3' conaffinity='1' user='1'/>
    <!-- <geom type='box' pos='3 0 -0.2' size='1 1 0.2' material='ground' condim='3' conaffinity='1' user='1'/> -->
    <!-- <geom type='box' pos='-3 0 -0.2' size='1 1 0.2' material='ground' condim='3' conaffinity='1' user='1'/> -->
    <!-- <geom type='box' pos='4.1 0 -0.1' size='0.1 1 0.2' material='ground' condim='3' conaffinity='1' user='1'/> -->
    <!-- <geom type='box' pos='4.3 0 -0.0' size='0.1 1 0.2' material='ground' condim='3' conaffinity='1' user='1'/> -->
    <!-- <geom type='box' pos='4.5 0 0.1' size='0.1 1 0.2' material='ground' condim='3' conaffinity='1' user='1'/> -->
    <!-- <geom type='box' pos='4.7 0 0.2' size='0.1 1 0.2' material='ground' condim='3' conaffinity='1' user='1'/> -->
    <light mode='fixed' directional='true' diffuse='.8 .8 .8' specular='0.3 0.3 0.3' pos='0 0 100.0' dir='0 0 -1'/>

    <body name='pelvis' pos='0 0 1'>
      <camera name='tracking' pos='41 -200 62.5' euler='0.2 0 1.2708' mode='trackcom' fovy='1'/>
      <joint type='slide' axis='1 0 0' limited='false'/>
      <joint type='slide' axis='0 0 1' ref='1' limited='false'/>
      <!-- <joint type='slide' axis='0 0 1' ref='1' limited='true' range='0.6 2'/> -->
      <!-- <joint type='hinge' axis='0 -1 0' limited='false'/> -->
      <joint type='hinge' axis='0 -1 0' limited='true' range='-1e-3 1e-3'/>
      <geom type='sphere' size='0.1' contype='1'/>
      <inertial pos='0 0 0' mass='30' diaginertia='0.3 0.3 0.3'/>
      <body name='right_hip' pos='0 -0.1 0'>
        <inertial pos='0 0 0' mass='1e-6' diaginertia='1e-6 1e-6 1e-6'/>
        <joint name='right_angle' type='hinge' axis='0 -1 0' range='-1.5 1.5' damping='0.1' armature='0.256'/>
        <geom type='capsule' fromto='0 0 0 0 0 -0.3' size='0.05'/>
        <body name='right_angle_spring' pos='0 0 0'>
          <inertial pos='0 0 0' mass='1e-6' diaginertia='1e-6 1e-6 1e-6'/>
          <joint name='right_angle_spring' type='hinge' axis='0 -1 0' limited='false' stiffness='1e4' damping='1e2'/>
          <body name='right_length_spring' pos='0 0 0'>
            <inertial pos='0 0 0' mass='1e-6' diaginertia='1e-6 1e-6 1e-6'/>
            <joint name='right_length' type='slide' axis='0 0 -1' range='0.3 1' ref='1' damping='0.1' armature='2.304'/>
            <body name='right_leg' pos='0 0 -1'>
              <inertial pos='0 0 0' mass='0.4' diaginertia='1e-6 1e-6 1e-6'/>
              <joint name='right_length_spring' type='slide' axis='0 0 -1' limited='false' stiffness='1e4' damping='1e2'/>
              <geom type='capsule' fromto='0 0 0.025 0 0 0.7' size='0.025' contype='1'/>
            </body>
          </body>
        </body>
      </body>
      <body name='left_hip' pos='0 0.1 0'>
        <inertial pos='0 0 0' mass='1e-6' diaginertia='1e-6 1e-6 1e-6'/>
        <joint name='left_angle' type='hinge' axis='0 -1 0' range='-1.5 1.5' damping='0.1' armature='0.256'/>
        <geom type='capsule' fromto='0 0 0 0 0 -0.3' size='0.05'/>
        <body name='left_angle_spring' pos='0 0 0'>
          <inertial pos='0 0 0' mass='1e-6' diaginertia='1e-6 1e-6 1e-6'/>
          <joint name='left_angle_spring' type='hinge' axis='0 -1 0' limited='false' stiffness='1e4' damping='1e2'/>
          <body name='left_length_spring' pos='0 0 0'>
            <inertial pos='0 0 0' mass='1e-6' diaginertia='1e-6 1e-6 1e-6'/>
            <joint name='left_length' type='slide' axis='0 0 -1' range='0.3 1' ref='1' damping='0.1' armature='2.304'/>
            <body name='left_leg' pos='0 0 -1'>
              <inertial pos='0 0 0' mass='0.4' diaginertia='1e-6 1e-6 1e-6'/>
              <joint name='left_length_spring' type='slide' axis='0 0 -1' limited='false' stiffness='1e4' damping='1e2'/>
              <geom type='capsule' fromto='0 0 0.025 0 0 0.7' size='0.025' contype='1'/>
            </body>
          </body>
        </body>
      </body>
    </body>

  </worldbody>

  <actuator>
    <!-- <position name='right_angle_p' joint='right_angle' kp='1e3' forcerange='-192 192'/> -->
    <!-- <velocity name='right_angle_d' joint='right_angle' kv='1e2' forcerange='-192 192'/> -->
    <!-- <position name='right_length_p' joint='right_length' kp='4e3' forcerange='-576 576'/> -->
    <!-- <velocity name='right_length_d' joint='right_length' kv='1e2' forcerange='-576 576'/> -->
    <!-- <position name='left_angle_p' joint='left_angle' kp='1e3' forcerange='-192 192'/> -->
    <!-- <velocity name='left_angle_d' joint='left_angle' kv='1e2' forcerange='-192 192'/> -->
    <!-- <position name='left_length_p' joint='left_length' kp='4e3' forcerange='-576 576'/> -->
    <!-- <velocity name='left_length_d' joint='left_length' kv='1e2' forcerange='-576 576'/> -->
    <motor name='right_angle_p' joint='right_angle' forcerange='-192 192'/>
    <motor name='right_length_p' joint='right_length' forcerange='-576 576'/>
    <motor name='left_angle_p' joint='left_angle' forcerange='-192 192'/>
    <motor name='left_length_p' joint='left_length' forcerange='-576 576'/>
  </actuator>

</mujoco>
